{
  "name": "beanstalkd-dashboard",
  "private": false,
  "version": "0.1.0",
  "type": "module",
  "bin": "./bin/cli.js",
  "files": [
    "./bin/cli.js",
    "./dist",
    "./dist-server"
  ],
  "scripts": {
    "start": "node dist-server/index.js",
    "dev": "vite",
